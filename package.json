{
  "name": "@atomico/components",
  "description": "Generic and utility webcomponents created with Atomico",
  "version": "1.2.0",
  "types": "*.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "vite",
    "build": "exports src/*.{js,jsx} --exports --types",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@atomico/hooks": "latest",
    "atomico": "latest"
  },
  "devDependencies": {
    "@atomico/exports": "latest",
    "typescript": "latest",
    "vite": "latest"
  },
  "exports": {
    "./modal": "./dest/modal.js",
    "./router": "./dest/router.js",
    "./send-form": "./dest/send-form.js"
  }
}
