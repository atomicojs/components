{
  "name": "@uppercod/components",
  "description": "Generic and utility webcomponents created with Atomico",
  "version": "0.0.3-beta",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./modal": "./dest/modal.js"
  },
  "scripts": {
    "start": "devserver dev src --jsxImportSource atomico",
    "build": "devserver build src/components/*/*.js dest --jsxImportSource atomico --external atomico,atomico/jsx-runtime,@atomico/hooks/use-responsive-state,@atomico/hooks/use-resize-observer --minify --minifyCssLiteral"
  },
  "dependencies": {
    "@atomico/hooks": "latest",
    "atomico": "latest"
  },
  "devDependencies": {
    "@devserver/build": "latest",
    "@devserver/cli": "latest"
  }
}
