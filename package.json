{
  "name": "@atomico/components",
  "description": "Generic and utility webcomponents created with Atomico",
  "version": "1.2.2",
  "types": "*.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "vite",
    "build": "exports src/*/*.{js,jsx} --exports --types",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@atomico/hooks": "latest",
    "atomico": "latest"
  },
  "devDependencies": {
    "@atomico/exports": "latest",
    "typescript": "latest",
    "vite": "latest"
  },
  "exports": {
    "./modal": "./dest/modal/modal.js",
    "./router": "./dest/router/router.js",
    "./send-form": "./dest/send-form/send-form.js"
  },
  "typesVersions": {
    "*": {
      "modal": [
        "modal/modal.d.ts"
      ],
      "router": [
        "router/router.d.ts"
      ],
      "send-form": [
        "send-form/send-form.d.ts"
      ]
    }
  }
}