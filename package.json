{
  "name": "@atomico/components",
  "description": "Generic and utility webcomponents created with Atomico",
  "version": "1.6.0",
  "types": "*.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "vite",
    "exports": "exports src/*/*.{js,jsx} --exports --types",
    "test": "wtr src/**/*.test.js --node-resolve",
    "prepublishOnly": "npm run test && npm run exports"
  },
  "dependencies": {
    "@atomico/hooks": "^3.26.3",
    "atomico": "^1.41.0"
  },
  "devDependencies": {
    "@atomico/exports": "^0.22.2",
    "@esm-bundle/chai": "^4.3.4",
    "@uppercod/vite-meta-url": "^1.3.1",
    "@web/test-runner": "^0.13.5",
    "typescript": "latest",
    "vite": "latest",
    "vite-web-test-runner-plugin": "^0.0.3"
  },
  "peerDependencies": {
    "atomico": "^1.41.0",
    "@atomico/hooks": "^3.26.3"
  }
}
