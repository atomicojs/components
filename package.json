{
  "name": "@atomico/components",
  "description": "Generic and utility webcomponents created with Atomico",
  "version": "1.4.0",
  "types": "*.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "vite",
    "build": "exports src/*/*.{js,jsx} --exports --types",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@atomico/hooks": "latest",
    "atomico": "latest"
  },
  "peerDependencies": {
    "@atomico/react": "latest"
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@atomico/exports": "^0.12.0",
    "@uppercod/vite-meta-url": "^1.3.1",
    "typescript": "latest",
    "vite": "latest"
  }
}
