{
  "name": "@atomico/modal",
  "description": "Webcomponent built with Atomico JS, Simple to use and customize modal. Includes additional support for React and Preact.",
  "version": "1.1.3",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "atomico": "latest",
    "@atomico/hooks": "latest"
  },
  "peerDependencies": {
    "atomico": "latest",
    "@atomico/hooks": "latest",
    "@atomico/react": "latest"
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    }
  },
  "scripts": {
    "component:publish": "exports src/{define,elements}.{ts,tsx} --types --exports --minify --publish --analyzer --main define"
  },
  "bugs": {
    "url": "https://github.com/atomicojs/components/issues"
  },
  "homepage": "https://atomico.gitbook.io/doc/atomico/atomico-components/modal"
}
