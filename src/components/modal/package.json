{
  "name": "@atomico/modal",
  "description": "Webcomponent built with Atomico JS, Simple to use and customize modal. Includes additional support for React and Preact.",
  "version": "1.3.3",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "atomico": "^1.70.0",
    "@atomico/hooks": "*",
    "@atomico/react": "*"
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    }
  },
  "scripts": {
    "build:types": "tsc",
    "build:compile": "library \"src/**/*\"",
    "build:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers --main define --publish",
    "dev:types": "tsc --watch",
    "dev:exports": "exports \"src/**/*\" \"types/**/*\""
  },
  "bugs": {
    "url": "https://github.com/atomicojs/components/issues"
  },
  "homepage": "https://atomico.gitbook.io/doc/atomico/atomico-components/modal",
  "types": "types/index.d.ts",
  "exports": {
    "./elements": {
      "default": "./src/elements.tsx",
      "types": "./types/elements.d.ts"
    },
    ".": {
      "default": "./src/index.ts",
      "types": "./types/index.d.ts"
    },
    "./stories/index.stories": "./src/stories/index.stories.tsx",
    "./style.css": {
      "default": "./src/stories/style.css"
    },
    "./tsconfig.json": {
      "default": "./src/stories/tsconfig.json"
    }
  },
  "typesVersions": {
    "*": {
      "elements": [
        "types/elements.d.ts"
      ]
    }
  }
}