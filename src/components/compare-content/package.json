{
  "name": "@atomico/compare-content",
  "version": "1.2.3",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "atomico": "^1.70.0",
    "@atomico/hooks": "*",
    "@atomico/react": "*"
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    }
  },
  "scripts": {
    "build:types": "tsc",
    "build:compile": "library \"src/**/*\"",
    "build:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers --main define --publish",
    "dev:types": "tsc --watch",
    "dev:exports": "exports \"src/**/*\" \"types/**/*\"",
    "component:publish": "npm run build:types && npm run build:compile && npm run build:exports"
  }
}
