{
  "name": "@atomico/compare-content",
  "version": "1.2.3",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "atomico": "^1.70.0",
    "@atomico/hooks": "*",
    "@atomico/react": "*"
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    }
  },
  "scripts": {
    "build:types": "tsc",
    "build:compile": "library \"src/**/*\"",
    "build:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers --main define --publish",
    "dev:types": "tsc --watch",
    "dev:exports": "exports \"src/**/*\" \"types/**/*\""
  },
  "exports": {
    "./elements": "./src/elements.tsx",
    ".": "./src/index.tsx",
    "./stories/index.stories": "./src/stories/index.stories.tsx"
  },
  "typesVersions": {
    "*": {
      "elements": [
        "types/elements.d.ts"
      ],
      "stories/index.stories": [
        "types/stories/index.stories.d.ts"
      ]
    }
  },
  "types": "types/index.d.ts"
}