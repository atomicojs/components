{
  "name": "@atomico/table",
  "description": "Webcomponent built with Atomico JS, Simple to use and customize table. Includes additional support for React and Preact.",
  "version": "1.2.4",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "atomico": "^1.70.0",
    "@atomico/hooks": "*",
    "@atomico/react": "*"
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    }
  },
  "scripts": {
    "step:types": "tsc",
    "step:build": "library \"src/**/*\"",
    "step:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers --main define --publish",
    "component:publish": "npm run step:types && npm run step:build && npm run step:exports"
  },
  "bugs": {
    "url": "https://github.com/atomicojs/components/issues"
  },
  "homepage": "https://atomico.gitbook.io/doc/atomico/atomico-components/table"
}
